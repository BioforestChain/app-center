{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ESNext",
    "outDir": "./dist",
    "useDefineForClassFields": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noImplicitOverride": true,
    "allowImportingTsExtensions": true,
    "lib": ["ESNext", "DOM"],
    "skipLibCheck": true,
    "noEmit": true,
    "paths": {
      "@plaoc/is-dweb": ["node_modules/@plaoc/is-dweb"],
      "@/*": ["src/*"]
    },
    "typeRoots": ["./node_modules/@plaoc/is-dweb/types"]
  },
  "include": ["src/**/*", "env.d.ts", "tmp/Contactus.vue"],
  "exclude": ["node_modules", "dist"]
}
